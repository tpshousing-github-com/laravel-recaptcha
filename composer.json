{
    "authors": [
        {
            "email": "roby.belotti@gmail.com",
            "homepage": "https://biscolab.com",
            "name": "Roberto Belotti",
            "role": "Developer"
        }
    ],
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Biscolab\\ReCaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Biscolab\\ReCaptcha\\Tests\\": "tests/"
        }
    },
    "description": "Simple and painless Google reCAPTCHA package for Laravel 5.5 or greater",
    "extra": {
        "laravel": {
            "aliases": {
                "ReCaptcha": "Biscolab\\ReCaptcha\\Facades\\ReCaptcha"
            },
            "providers": [
                "Biscolab\\ReCaptcha\\ReCaptchaServiceProvider"
            ]
        }
    },
    "homepage": "https://biscolab.com/laravel-recaptcha",
    "keywords": [
        "recaptcha",
        "captcha",
        "laravel",
        "validation"
    ],
    "license": "MIT",
    "name": "tpshousing-github-com/laravel-recaptcha",
    "require": {
        "laravel/framework": "^5.5|^6.0|^7.0|^8.0",
        "php": "^7.1|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "6.*|7.*"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "suggest": {
        "biscolab/laravel-authlog": "It allows to handle logged-in users and force log-out if needed"
    },
    "type": "library",
    "version": "4.4.0"
}
